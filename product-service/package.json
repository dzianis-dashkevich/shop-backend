{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .ts src test --max-warnings 0 --fix",
    "offline": "serverless offline start --stage dev",
    "deploy:dev": "serverless create_domain --stage dev && serverless deploy --stage dev",
    "deploy:prod": "serverless create_domain --stage prod && serverless deploy --stage prod"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^28.1.6",
    "@types/node": "^14.14.25",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "joi": "^17.6.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.8.2",
    "serverless-domain-manager": "^6.0.3",
    "serverless-offline": "^9.0.0",
    "serverless-webpack": "^5.7.1",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.1.3",
    "webpack": "^5.73.0"
  },
  "license": "ISC",
  "dependencies": {
    "pg": "^8.7.3",
    "uuid": "^8.3.2"
  }
}
